{
  "name": "openapi-to-normalizr",
  "description": "generator for normalizr",
  "version": "0.1.5",
  "author": {
    "name": "Eight",
    "url": "https://github.com/eightcard/openapi-to-normalizr"
  },
  "bin": {
    "openapi2schemas": "./bin/generateschemas"
  },
  "engines": {
    "node": ">=10.15.1"
  },
  "dependencies": {
    "commander": "4.0.1",
    "js-yaml": "3.13.1",
    "json-schema-ref-parser": "7.1.2",
    "lodash": "4.17.15",
    "mkdirp": "0.5.1",
    "mustache": "3.1.0",
    "swagger-client": "3.9.6"
  },
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.5",
    "@babel/preset-typescript": "7.7.4",
    "@babel/register": "7.7.4",
    "@types/js-yaml": "3.12.1",
    "@types/json-schema": "7.0.3",
    "@types/lodash": "4.14.149",
    "@types/mkdirp": "0.5.2",
    "@types/prop-types": "15.7.3",
    "@types/react-immutable-proptypes": "2.1.0",
    "@types/redux-actions": "2.6.1",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-polyfill": "6.26.0",
    "babel-preset-power-assert": "3.0.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "immutable": "4.0.0-rc.12",
    "jest": "24.9.0",
    "json-server": "0.15.1",
    "lint-staged": "9.5.0",
    "nock": "11.7.0",
    "normalizr": "3.4.1",
    "openapi-types": "1.3.5",
    "power-assert": "1.6.1",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "react-immutable-proptypes": "2.1.0",
    "redux": "4.0.4",
    "rimraf": "3.0.0",
    "serve": "11.2.0",
    "speccy": "0.11.0",
    "typescript": "3.7.3",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10"
  },
  "peerDependencies": {
    "immutable": "^4.0.0-rc",
    "normalizr": "^3.2.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-checkout": "yarn",
      "post-merge": "yarn"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "config",
    "src",
    "dist",
    "templates",
    "examples",
    "bin",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "generator",
    "open api",
    "swagger"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "eightcard/openapi-to-normalizr",
  "scripts": {
    "build:dist": "yarn eslint && yarn test && tsc && babel -d dist --env-name=development src/lib/**/*[^test].js",
    "build:sample": "webpack --config examples/webpack.config.babel.js --progress",
    "build:sample:schemas": "./bin/generateschemas --config examples/config/config.schemas.js",
    "check:typescript": "tsc --noEmit",
    "eslint": "eslint src bin/*",
    "mock": "json-server --watch examples/db.json --port 10010",
    "prepare:mock": "cd examples/petstore && yarn",
    "serve": "bin/serve examples ",
    "test": "jest src/lib spec && yarn check:typescript",
    "jest": "jest",
    "speclint": "find spec examples -name '*.yml' | xargs -I{} yarn speccy {}",
    "speccy": "speccy lint -s 'operation-operationId' "
  }
}
