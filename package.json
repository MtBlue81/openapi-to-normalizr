{
  "name": "openapi-to-normalizr",
  "description": "generator for normalizr",
  "version": "0.1.5",
  "author": {
    "name": "Eight",
    "url": "github.com/eightcard/openapi-to-normalizr"
  },
  "bin": {
    "openapi2schemas": "./bin/generateschemas"
  },
  "dependencies": {
    "commander": "3.0.0",
    "js-yaml": "3.13.1",
    "json-schema-ref-parser": "7.1.0",
    "lodash": "4.17.15",
    "mkdirp": "0.5.1",
    "mustache": "3.0.1",
    "swagger-client": "3.9.3"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/register": "7.5.5",
    "@types/lodash": "4.14.137",
    "@types/prop-types": "15.7.1",
    "@types/redux-actions": "2.6.1",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-polyfill": "6.26.0",
    "babel-preset-power-assert": "3.0.0",
    "eslint": "6.2.2",
    "flow-bin": "0.106.1",
    "flow-typed": "2.6.1",
    "immutable": "4.0.0-rc.12",
    "jest": "24.9.0",
    "json-server": "0.15.0",
    "nock": "10.0.6",
    "normalizr": "3.4.1",
    "power-assert": "1.6.1",
    "prop-types": "15.7.2",
    "react-immutable-proptypes": "2.1.0",
    "redux": "4.0.4",
    "rimraf": "3.0.0",
    "serve": "11.1.0",
    "speccy": "0.11.0",
    "typescript": "3.5.3",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7"
  },
  "peerDependencies": {
    "immutable": "^4.0.0-rc",
    "normalizr": "^3.2.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.0"
  },
  "files": [
    "config",
    "src",
    "dist",
    "templates",
    "examples",
    "bin",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "generator",
    "open api",
    "swagger"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "eightcard/openapi-to-normalizr",
  "scripts": {
    "build:dist": "yarn eslint && yarn test && babel -d dist --env-name=development src/lib/**/*[^test].js",
    "build:sample": "webpack --config examples/webpack.config.babel.js --progress",
    "build:sample:schemas": "./bin/generateschemas --config examples/config/config.schemas.js",
    "check:typescript": "tsc --noEmit",
    "eslint": "eslint src bin/*",
    "mock": "json-server --watch examples/db.json --port 10010",
    "prepare:mock": "cd examples/petstore && yarn",
    "serve": "bin/serve examples ",
    "test": "jest src/lib spec && yarn check:typescript",
    "jest": "jest",
    "flow": "flow tmp",
    "speclint": "find spec examples -name '*.yml' | xargs -I{} yarn speccy {}",
    "speccy": "speccy lint -s 'operation-operationId' "
  }
}
